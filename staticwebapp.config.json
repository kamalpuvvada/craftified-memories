{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico,svg}", "/css/*", "/js/*", "/*.{css,js,png,jpg,gif,ico,svg}","/api/"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
