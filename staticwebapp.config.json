{
  "routes": [
    {
      "route": "/api/*",
      "allowedMethods": ["GET", "POST", "OPTIONS", "PUT", "DELETE"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/static/*", 
      "/images/*", 
      "/*.{css,js,png,jpg,gif,ico,svg,json}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
