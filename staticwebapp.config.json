{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/static/*", 
      "/images/*", 
      "/*.{css,js,png,jpg,gif,ico,svg,json}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
